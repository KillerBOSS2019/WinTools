              {
                "id": "KillerBOSS.TP.Plugins.VolumeMixer.Increase/DecreaseVolume.Ince/DecreChoice",
                "type": "choice",
                "label": "choice",
                "default": "Increase",
                "valueChoices": [
                    "Increase",
                    "Decrease",
                    "Set"
                ]
              },
              {
                "id": "KillerBOSS.TP.Plugins.VolumeMixer.Increase/DecreaseVolume.Volume",
                "type": "number",
                "label": "choice",
                "allowDecimals": false,
                "minValue": 0,
                "maxValue": 100,
                "default": 10
              }
            ]
          },
          {
            "id": "KillerBOSS.TP.Plugins.AdvanceMouse.HoldDownToggle",
            "name": "Mouse: Toggle Mouse Down/Up",
