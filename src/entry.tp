            {
              "id": "KillerBOSS.TP.Plugins.VolumeMixer.Mute/Unmute.Choice",
              "type": "choice",
              "label": "choice",
              "default": "Mute",
              "valueChoices": [
                  "Mute",
                  "Unmute"
              ]
            }
          ]
        },
        {
            "id": "KillerBOSS.TP.Plugins.VolumeMixer.Increase/DecreaseVolume",
            "name": "Volume Mixer: Increase/Decrease process volume",
            "prefix": "plugin",
            "type": "communicate",
            "tryInline": true,
            "format": "{$KillerBOSS.TP.Plugins.VolumeMixer.Increase/DecreaseVolume.Ince/DecreChoice$}{$KillerBOSS.TP.Plugins.VolumeMixer.Increase/DecreaseVolume.process$}Volume to {$KillerBOSS.TP.Plugins.VolumeMixer.Increase/DecreaseVolume.Volume$}",
            "hasHoldFunctionality": true,
            "data": [
              {
                "id": "KillerBOSS.TP.Plugins.VolumeMixer.Increase/DecreaseVolume.process",
                "type": "choice",
                "label": "choice",
                "default": "",
                "valueChoices": []
