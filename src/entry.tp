            "data": [
              {
                "id": "KillerBOSS.TP.Plugins.AdvanceMouse.HoldDownToggle.Toggle",
                "type": "choice",
                "label": "choice",
                "default": "Down",
                "valueChoices": [
                    "Down",
                    "Up"
                ]
              },
              {
                "id": "KillerBOSS.TP.Plugins.AdvanceMouse.HoldDownToggle.ButtonPick",
                "type": "choice",
                "label": "choice",
                "default": "Left",
                "valueChoices": [
                    "Left",
                    "Right"
