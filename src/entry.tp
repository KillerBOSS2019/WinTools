            {
              "id": "KillerBOSS.TP.Plugins.VolumeMixer.slidercontrol",
              "type": "choice",
              "label": "choice",
              "default": "",
              "valueChoices": []
            }
          ]
        }
      ],
      "actions": [
        {
          "id": "KillerBOSS.TP.Plugins.VolumeMixer.Mute/Unmute",
          "name": "Volume Mixer: Mute/Unmute process volume",
          "prefix": "plugin",
          "type": "communicate",
          "tryInline": true,
          "format": "{$KillerBOSS.TP.Plugins.VolumeMixer.Mute/Unmute.Choice$} Program:{$KillerBOSS.TP.Plugins.VolumeMixer.Mute/Unmute.process$}",
          "data": [
            {
              "id": "KillerBOSS.TP.Plugins.VolumeMixer.Mute/Unmute.process",
              "type": "choice",
              "label": "choice",
              "default": "",
              "valueChoices": []
