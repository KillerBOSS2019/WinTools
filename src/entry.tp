            "prefix": "plugin",
            "type": "communicate",
            "tryInline": true,
            "hasHoldFunctionality": true,
            "format": "Increase Mouse X:{$KillerBOSS.TP.Plugins.AdvanceMouse.Function.X$} Y:{$KillerBOSS.TP.Plugins.AdvanceMouse.Function.Y$}",
            "data": [
              {
                "id": "KillerBOSS.TP.Plugins.AdvanceMouse.Function.X",
                "type": "number",
                "label": "number",
                "default": 10,
                "allowDecimals": false
              },
              {
                "id": "KillerBOSS.TP.Plugins.AdvanceMouse.Function.Y",
                "type": "number",
                "label": "number",
                "default": 10,
                "allowDecimals": false
              }
            ]
        },
        {
            "id": "KillerBOSS.TP.Plugins.AdvanceMouse.teleport",
            "name": "Mouse: Teleport Mouse X, Y with delay, animation",
            "prefix": "plugin",
            "type": "communicate",
            "tryInline": true,
            "format": "Mouse: Teleport Mouse to X:{$KillerBOSS.TP.Plugins.AdvanceMouse.teleport.X$} Y:{$KillerBOSS.TP.Plugins.AdvanceMouse.teleport.Y$} with animation Type:{$KillerBOSS.TP.Plugins.AdvanceMouse.teleport.animation$} with speed:{$KillerBOSS.TP.Plugins.AdvanceMouse.teleport.Speed$}",
            "data": [
              {
                "id": "KillerBOSS.TP.Plugins.AdvanceMouse.teleport.X",
                "type": "number",
                "label": "number",
                "default": 10,
                "allowDecimals": false
              },
              {
                "id": "KillerBOSS.TP.Plugins.AdvanceMouse.teleport.Y",
                "type": "number",
                "label": "number",
                "default": 10,
                "allowDecimals": false
              },
              {
                "id": "KillerBOSS.TP.Plugins.AdvanceMouse.teleport.animation",
                "type": "choice",
                "label": "choice",
                "default": "None",
                "valueChoices": [
                    "Start slow, end fast",
                    "Start fast, end slow",
                    "Start and end fast, slow in middle",
                    "bounce at the end",
                    "rubber band at the end",
                    "None"
                ]
              },
              {
                "id": "KillerBOSS.TP.Plugins.AdvanceMouse.teleport.Speed",
                "type": "number",
                "label": "number",
                "default": 10,
                "allowDecimals": false
              }
            ]
        },
        {
            "id": "KillerBOSS.TP.Plugins.AdvanceMouse.MouseClick",
            "name": "Mouse: Advanced Mouse Click",
            "prefix": "plugin",
            "type": "communicate",
            "tryInline": true,
            "hasHoldFunctionality": true,
            "format": "Mouse: Click {$KillerBOSS.TP.Plugins.AdvanceMouse.MouseClick.Times$}Times with delay in between{$KillerBOSS.TP.Plugins.AdvanceMouse.MouseClick.delay$}Seconds with Mouse Key:{$KillerBOSS.TP.Plugins.AdvanceMouse.MouseClick.ButtonPick$}",
            "data": [
                {
                "id": "KillerBOSS.TP.Plugins.AdvanceMouse.MouseClick.Times",
                "type": "number",
                "label": "number",
                "default": 1,
                "maxValue": 50,
                "minValue": 1,
                "allowDecimals": false
                },
                {
                    "id": "KillerBOSS.TP.Plugins.AdvanceMouse.MouseClick.delay",
                    "type": "number",
                    "label": "number",
                    "default": 0,
                    "maxValue": 60,
                    "minValue": 0,
                    "allowDecimals": true
