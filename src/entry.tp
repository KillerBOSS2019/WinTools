{
  "sdk": 6,
  "version": 231,
  "name": "Windows-Tools",
  "id": "Windows-Tools",
  "configuration": {
    "colorDark": "#6c6f73",
    "colorLight": "#3d62ad"
  },
  "settings": [
    {
      "name": "Update Interval: Public IP",
      "type": "number",
      "default": "240",
      "maxValue": 1800,
      "minValue": 0,
      "allowDecimals": false,
      "readOnly": false
    },
    {
      "name": "Update Interval: Hard Drive",
      "type": "number",
      "default": "60",
      "maxValue": 1800,
      "minValue": 0,
      "allowDecimals": false,
      "readOnly": false
    },
    {
      "name": "Update Interval: Active Monitors",
      "type": "number",
      "default": "240",
      "maxValue": 1800,
      "minValue": 0,
      "allowDecimals": false,
      "readOnly": false
    },
    {
      "name": "Update Interval: Active Windows",
      "type": "number",
      "default": "10",
      "maxValue": 1800,
      "minValue": 5,
      "allowDecimals": false,
      "readOnly": false
    },
    {
      "name": "Update Interval: Active Virtual Desktops",
      "type": "number",
      "default": "15",
      "maxValue": 1800,
      "minValue": 5,
      "allowDecimals": false,
      "readOnly": false
    },
    {
      "name": "Clipboard Listener (ON / OFF)",
      "type": "text",
      "default": "ON",
      "readOnly": false
    },
    {
      "name": "Audio State Exemption List",
      "type": "text",
      "default": "Enter '.exe' name seperated by a comma for more than 1",
      "readOnly": false
    },
    {
      "name": "DEBUG MODE / LOGGING",
      "type": "text",
      "description": "Wish we could write descript here",
      "default": "OFF",
      "readOnly": false
    }
  ],
  "plugin_start_cmd": "%TP_PLUGIN_FOLDER%WinTools\\WinTools.exe",
  "categories": [
    {
      "id": "main",
      "name": "Windows-Tool",
	    "imagepath": "%TP_PLUGIN_FOLDER%WinTools\\icon.png",
